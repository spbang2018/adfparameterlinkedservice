{
	"name": "Dynamic_Parameterize",
	"properties": {
		"description": "Dynamic_Parameterize",
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": {
							"value": "@{truncate table pipeline().parameters.dbtable}",
							"type": "Expression"
						},
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "az_destination_blob_dataset_copy2_parameters",
						"type": "DatasetReference",
						"parameters": {
							"filename": {
								"value": "@pipeline().parameters.Inputfile",
								"type": "Expression"
							},
							"containerName": {
								"value": "@pipeline().parameters.containername",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "az_source_db_connection_dataset_Parameter",
						"type": "DatasetReference",
						"parameters": {
							"tablename": {
								"value": "@pipeline().parameters.dbtable",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"Inputfile": {
				"type": "string"
			},
			"dbtable": {
				"type": "string"
			},
			"containername": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Parameterize"
		},
		"annotations": []
	}
}